<%- include('header_login') %>
<script src="js/jquery1.9.1.js"></script>
<script src="js/bootstrap.js"> </script>   
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <!-- login  Start-->

    
    <!-- login End-->



    <body class="skin-default card-no-border">
        <!-- ============================================================== -->
        <!-- Preloader - style you can find in spinners.css -->
        <!-- ============================================================== -->
        <div class="preloader">
            <div class="loader">
                <div class="loader__figure"></div>
                <p class="loader__label">Elite admin</p>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- Main wrapper - style you can find in pages.scss -->
        <!-- ============================================================== -->
        <section id="wrapper">
            <div class="login-register" style="background-image:url(assets/images/background/login-register.jpg);">
                <div class="login-box card">
                    <div class="card-body">
                        <!-- method ="post"action="/api/login/user" onsubmit="userlogin()" -->
                        <form class="form-horizontal form-material" id="loginform" method ="post"action="login">
                            <h3 class="text-center m-b-20">Sign In</h3>
                            <div class="form-group ">
                                <div class="col-xs-12">
                                    <% if (locals.error) { %>
                                        <div class="alert alert-danger" role="alert">
                                          <%= locals.error %>
                                        </div>
                                      <% } %>
                                    <input type="email" name="email" placeholder="Email address" class="form-control form-control-user" id="email"  > </div>
                            </div>
                            <div class="form-group">
                                <div class="col-xs-12">
                                    <input type="password" name="password" placeholder="Password" class="form-control" id="password"></div>
                                    <% if (locals.errors) { %>
                                        <div class="alert alert-danger" role="alert">
                                          <%= locals.errors %>
                                        </div>
                                      <% } %>
                            </div>
                            <div class="form-group row">
                                <div class="col-md-12">
                                    <div class="d-flex no-block align-items-center">
                                        <div class="custom-control custom-checkbox">
                                            <input type="checkbox" class="custom-control-input" id="customCheck1">
                                            <label class="custom-control-label" for="customCheck1">Remember me</label>
                                        </div> 
                                
                                    </div>
                                </div>
                            </div>
                            <div class="form-group text-center">
                                <div class="col-xs-12 p-b-20">
                                 <input  class="btn btn-block btn-lg btn-info btn-rounded" type="submit" value="Submit">
                                    <!-- <buttton href="#" onclick="userlogin(this)" class="btn btn-block btn-lg btn-info btn-rounded" ;>Login </buttton> -->
                                </div>
                            </div>
                  
                            <div class="form-group m-b-0">
                                <div class="col-sm-12 text-center">
                                    Don't have an account? <a href="/register" class="text-info m-l-5"><b>Sign Up</b></a>
                                </div>
                            </div>
                        </form>
                   
                     
                      
                    </div>
                </div>
            </div>
        </section>
        
        <!-- ============================================================== -->
        <!-- End Wrapper -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- All Jquery -->
        <!-- ============================================================== -->
        <script language="JavaScript"  src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.0/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.min.js"></script>
<!-- Register End-->

<!-- Bootstrap tether Core JavaScript -->
<script src="assets/node_modules/popper/popper.min.js"></script>
<script src="assets/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>
        <script src="assets/node_modules/jquery/jquery-3.2.1.min.js"></script>
        <!-- Bootstrap tether Core JavaScript -->
        <script src="assets/node_modules/popper/popper.min.js"></script>
        <script src="assets/node_modules/bootstrap/dist/js/bootstrap.min.js"></script>


        <style>
            label.error {
              color: red;
            }
            </style>
                <script>
                    $(document).ready(function() {
                      $('#loginform').validate({
                        rules: {
                          email: {
                            required: true,
                            email: true
                          },
                          password: {
                            required: true,
                            minlength: 4
                          }
                        },
                        messages: {
                          email: {
                            required: "Please enter your email address",
                            email: "Please enter a valid email address"
                          },
                          password: {
                            required: "Please enter a password",
                            minlength: "Your password must be at least 8 characters long"
                          }
                        }
                      });
                    });
                    </script>
        <!--Custom JavaScript -->
        <script type="text/javascript">
            $(function() {
                $(".preloader").fadeOut();
            });
            $(function() {
                $('[data-toggle="tooltip"]').tooltip()
            });
            // ============================================================== 
            // Login and Recover Password 
            // ============================================================== 
            $('#to-recover').on("click", function() {
                $("#loginform").slideUp();
                $("#recoverform").fadeIn();
            });
        </script>
        
    </body>




    <script src="assets/js/pages/login.js"></script>
    <%- include('footer') %>